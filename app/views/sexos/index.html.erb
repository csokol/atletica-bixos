<h1>Listing sexos</h1>

<table>
  <tr>
    <th>Tipo</th>
  </tr>

<% for sexo in @sexos %>
  <tr>
    <td><%=h sexo.tipo %></td>
    <td><%= link_to 'Show', sexo %></td>
    <td><%= link_to 'Edit', edit_sexo_path(sexo) %></td>
    <td><%= link_to 'Destroy', sexo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sexo', new_sexo_path %>
